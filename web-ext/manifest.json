{
	"manifest_version": 2,
	"name": "cf-rating-predictor",
	"version": "0.0",

	"description": "Predict Codeforces Rating during Contests",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2.png"
	},

	"content_scripts": [
		{
			"matches": ["*://codeforces.com/contest/*/standings*"],
			"js": ["main.js"]
		}
	]
}
